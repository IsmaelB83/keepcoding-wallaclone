{
  "name": "backend-wallaclone-backend",
  "version": "1.0.0",
  "description": "Backend created for the wallaclone app",
  "main": "./src/index.js",
  "scripts": {
    "init": "node ./src/database/install_db.js",
    "start": "node ./src/index.js",
    "debug": "nodemon --nolazy --inspect-brk=9543 ./src/index.js",
    "debugPhotos": "nodemon --nolazy --inspect-brk=9544 ./src/services/workers/photos.js",
    "debugNotify": "nodemon --nolazy --inspect-brk=9545 ./src/services/workers/notify.js",
    "debugWebpush": "nodemon --nolazy --inspect-brk=9546 ./src/services/workers/testpush.js",
    "prod": "cross-env NODE_ENV=production npm start",
    "photos": "node ./src/services/workers/photos.js",
    "notify": "node ./src/services/workers/notify.js",
    "chat": "nodemon ./src/services/chat/index.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/IsmaelB83/keepcoding-wallafit"
  },
  "keywords": [
    "nodejs",
    "express",
    "mongoose",
    "api",
    "apirest",
    "restfull",
    "database"
  ],
  "author": "Ismael Bernal",
  "license": "ISC",
  "devDependencies": {
    "cross-env": "^6.0.3",
    "eslint": "^6.0.1",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.9.0",
    "nodemon": "^1.19.1",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "amqplib": "^0.5.5",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ejs": "^2.6.2",
    "ejs-promise": "^0.3.3",
    "express": "^4.17.1",
    "express-validator": "^6.1.1",
    "html-to-text": "^5.1.1",
    "i18n": "^0.8.4",
    "is-valid-username": "0.0.1",
    "jimp": "^0.8.5",
    "jsonwebtoken": "^8.5.1",
    "juice": "^5.2.0",
    "moment": "^2.24.0",
    "mongoose": "^5.7.5",
    "mongoose-url-slugs": "^1.0.2",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "nodemailer": "^6.3.1",
    "socket.io": "^2.3.0",
    "web-push": "^3.4.3"
  }
}
